{
  "name": "angular-webworker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "http-server dist -p 4200",
    "build": "./node_modules/webpack/bin/webpack.js --env.client -d",
    "build:aot": "npm run clean:dist && ./node_modules/webpack/bin/webpack.js --env.aot --env.client",
    "clean:dist": "rimraf dist",
    "watch": "./node_modules/webpack/bin/webpack.js --env.client -d --watch",
    "test": "karma start ./karma.conf.js",
    "test:watch": "karma start ./karma.conf.js --auto-watch --no-single-run",
    "lint": "tslint --type-check --project tsconfig.app.json",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "e2e": "webdriver-manager update && npm-run-all -p -r e2e:server e2e:live",
    "e2e:live": "protractor ./protractor.conf.js",
    "e2e:server": "http-server dist -p 4200 -c-1 --cors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0-beta.6",
    "@angular/common": "^5.0.0-beta.6",
    "@angular/compiler": "^5.0.0-beta.6",
    "@angular/core": "^5.0.0-beta.6",
    "@angular/forms": "^5.0.0-beta.6",
    "@angular/http": "^5.0.0-beta.6",
    "@angular/platform-browser": "^5.0.0-beta.6",
    "@angular/platform-browser-dynamic": "^5.0.0-beta.6",
    "@angular/platform-webworker": "^5.0.0-beta.6",
    "@angular/platform-webworker-dynamic": "^5.0.0-beta.6",
    "@angular/router": "^5.0.0-beta.6",
    "@angularclass/bootloader": "^1.0.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.27",
    "@angular/compiler-cli": "^5.0.0-beta.6",
    "@angular/language-service": "^5.0.0-beta.6",
    "@ngtools/webpack": "^1.6.2",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.34",
    "@types/webpack": "^3.0.13",
